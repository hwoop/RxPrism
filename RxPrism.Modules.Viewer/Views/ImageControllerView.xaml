<cv:RxUserControlBase xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                      xmlns:core="clr-namespace:RxPrism.Core;assembly=RxPrism.Core"
                      xmlns:cv="clr-namespace:RxPrism.Core.Views;assembly=RxPrism.core"
                      xmlns:vm="clr-namespace:RxPrism.Modules.Viewer.ViewModels"
                      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                      x:TypeArguments="vm:ImageControllerViewModel"
                      x:Class="RxPrism.Modules.Viewer.Views.ImageControllerView"
                      xmlns:prism="http://prismlibrary.com/"
                      prism:ViewModelLocator.AutoWireViewModel="True"
                      MinWidth="300" MinHeight="300">
    <Grid Background="AliceBlue">
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>

        <ContentControl prism:RegionManager.RegionName="{x:Static core:RegionNames.ImageRegion}"
                        Padding="10"
                        Background="LightPink"/>
    </Grid>
</cv:RxUserControlBase>
